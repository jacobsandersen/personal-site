---
import { Like } from '~/lib/content'
import ExternalLink from '~/components/util/ExternalLink.astro'

interface Props {
    like: Like
}

const { like } = Astro.props as Props
---

<div class="tw:flex tw:flex-col">
    <h2 class="tw:text-xl tw:font-bold">Liked <em><ExternalLink href={like.likeOf} showIcon={true}>a post</ExternalLink></em></h2>
</div>
